<ol class="{$class}" n:if="$items" n:inner-foreach="$items as $item">
	<li n:if="!$iterator->last">
		<a n:class="$ajax ? ajax" href="{$item->isUrl() ? $item->getLink() : $presenter->link($item->getLink(), $item->getLinkArgs())}">
			<i n:if="$item->getIcon()" class="fa fa-fw {$item->getIcon()}"></i>{_$item->getLabel()}
		</a>
	</li>
	<li class="active" n:if="$iterator->last">
		<i n:if="$item->getIcon()" class="fa fa-fw {$item->getIcon()}"></i>
		{_$item->getLabel()}
	</li>
</ol>
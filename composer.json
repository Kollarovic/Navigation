{
	"name": "kollarovic/navigation",
	"description": "Navigation for Nette",
	"keywords": ["nette", "navigation"],
	"license": "MIT",

	"authors": [
		{
			"name": "Mario Kollarovic",
			"email": "kollarovic@gmail.com"
		}
	],

	"require": {
		"php": ">= 8.0",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.2",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.1",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0"
	},

	"require-dev": {
		"phpstan/phpstan-nette": "^1.2",
		"nette/tester": "^2.5"
	},

	"autoload": {
		"psr-0": {
			"Kollarovic\\Navigation": "src/"
		}
	},

	"scripts": {
		"phpstan": "vendor/bin/phpstan analyse -c phpstan.neon src --level 7",
		"test": "vendor/bin/tester tests"
	}

}
